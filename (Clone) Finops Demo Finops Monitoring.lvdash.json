{
  "datasets": [
    {
      "name": "ds_counter_rec_rev_30d",
      "displayName": "KPI: Recognized Revenue Last 30d",
      "queryLines": [
        "SELECT total_recognized_revenue_last_30d FROM fin_ops.biz.gold_counter_recognized_revenue_last_30d "
      ]
    },
    {
      "name": "ds_counter_actual_spend_30d",
      "displayName": "KPI: Actual Supplier Spend Last 30d",
      "queryLines": [
        "SELECT total_actual_spend_last_30d FROM fin_ops.biz.gold_counter_actual_spend_last_30d "
      ]
    },
    {
      "name": "ds_counter_comm_rev_90d",
      "displayName": "KPI: Committed Revenue Next 90d",
      "queryLines": [
        "SELECT date, region, product_line, committed_revenue_next_90d FROM fin_ops.biz.gold_contract_commitments ORDER BY date DESC "
      ]
    },
    {
      "name": "ds_counter_budget_variance",
      "displayName": "KPI: Budget Variance To Date",
      "queryLines": [
        "SELECT budget_variance_to_date_usd FROM fin_ops.biz.gold_counter_budget_variance_to_date "
      ]
    },
    {
      "name": "ds_rev_vs_forecast_timeseries",
      "displayName": "Daily Recognized vs Forecast by Region",
      "queryLines": [
        "SELECT date, region, product_line, 'recognized' AS metric_type, recognized_revenue_usd AS value_usd, CASE WHEN date BETWEEN DATE('2025-07-16') AND DATE('2025-08-10') THEN 1 ELSE 0 END AS is_event_window FROM fin_ops.biz.gold_financial_timeseries UNION ALL SELECT date, region, product_line, 'forecast' AS metric_type, forecast_revenue_usd AS value_usd, CASE WHEN date BETWEEN DATE('2025-07-16') AND DATE('2025-08-10') THEN 1 ELSE 0 END AS is_event_window FROM fin_ops.biz.gold_financial_timeseries ORDER BY date, region, metric_type "
      ]
    },
    {
      "name": "ds_spend_weekly_long",
      "displayName": "Weekly Spend Actual vs Committed by Supplier Tier",
      "queryLines": [
        "SELECT week_start, region, product_line, supplier_tier, 'actual' AS metric_type, actual_spend_usd AS value_usd, approval_change_code_rate FROM fin_ops.biz.gold_supplier_spend_weekly UNION ALL SELECT week_start, region, product_line, supplier_tier, 'committed' AS metric_type, committed_spend_usd AS value_usd, approval_change_code_rate FROM fin_ops.biz.gold_supplier_spend_weekly ORDER BY week_start, region, product_line, supplier_tier, metric_type "
      ]
    },
    {
      "name": "ds_change_log",
      "displayName": "Root Cause Log",
      "queryLines": [
        "SELECT change_date, change_type, scope_region, product_line, supplier_tier_or_customer, description, exception_code FROM fin_ops.biz.gold_change_log ORDER BY change_date "
      ]
    },
    {
      "name": "ds_budget_variance",
      "displayName": "Daily Budget Variance",
      "queryLines": [
        "SELECT date, region, product_line, variance_usd, cumulative_variance_usd FROM fin_ops.biz.gold_budget_variance ORDER BY date, region "
      ]
    },
    {
      "name": "ds_cash_flow",
      "displayName": "Daily Cash Flow",
      "queryLines": [
        "SELECT date, region, product_line, net_cash_flow_usd, cumulative_net_cash_flow_usd FROM fin_ops.biz.gold_cash_flow_timeseries ORDER BY date, region, product_line "
      ]
    },
    {
      "name": "ds_spend_daily_share",
      "displayName": "Daily Supplier Spend Share",
      "queryLines": [
        "SELECT date, supplier_tier, spend_usd, share FROM fin_ops.biz.gold_supplier_spend_daily_share ORDER BY date, supplier_tier "
      ]
