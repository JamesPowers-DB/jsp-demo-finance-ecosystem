{
  "datasets": [
    {
      "name": "ds_headcount_metrics",
      "displayName": "Headcount Productivity Metrics",
      "queryLines": [
        "SELECT   \n",
        "  year,  \n",
        "  quarter,  \n",
        "  CONCAT('FY',CAST(year AS STRING), 'Q',CAST(quarter AS STRING)) AS fiscal_quarter_name,\n",
        "  cost_center,  \n",
        "  legal_entity,  \n",
        "  MEASURE(quarterly_headcount_cost) AS headcount_cost,  \n",
        "  MEASURE(quarterly_revenue) AS revenue,  \n",
        "  MEASURE(revenue_per_employee) AS revenue_per_employee,  \n",
        "  MEASURE(revenue_per_sales_employee) AS revenue_per_sales,  \n",
        "  MEASURE(quarterly_spend) AS spend,  \n",
        "  MEASURE(spend_per_employee) AS spend_per_employee\n",
        "FROM main.finance_lakehouse.metric_headcount\n",
        "GROUP BY \n",
        "all"
      ]
    },
    {
      "name": "ds_variance_performance",
      "displayName": "Variance & Performance Metrics",
      "queryLines": [
        "SELECT   \n",
        "  fiscal_year,  \n",
        "  fiscal_quarter,  \n",
        "  fiscal_quarter_name,  \n",
        "  cost_center,  \n",
        "  legal_entity,  \n",
        "  MEASURE(budget_amount) AS budget,  \n",
        "  MEASURE(actual_amount) AS actuals, \n",
        "  MEASURE(variance_amount) AS variance,  \n",
        "  MEASURE(variance_percent) AS variance_pct,  \n",
        "  MEASURE(actual_spend) AS actual_spend,  \n",
        "  MEASURE(actual_salary) AS actual_salary\n",
        "FROM main.finance_lakehouse.metric_planning\n",
        "GROUP BY ALL"
      ]
    },
    {
      "name": "ds_cash_obligations",
      "displayName": "Cash & Obligations Metrics",
      "queryLines": [
        "SELECT   \n",
        "  invoice_year,  \n",
        "  invoice_month,  \n",
        "  invoice_status, \n",
        "  supplier,  \n",
        "  legal_entity,  \n",
        "  contract_status,  \n",
        "  agreement_type,  \n",
        "  MEASURE(total_invoice_amount) AS invoice_amount,  \n",
        "  MEASURE(amount_paid) AS paid,  \n",
        "  MEASURE(outstanding_payables) AS outstanding,  \n",
        "  MEASURE(days_payable_outstanding) AS dpo,  \n",
        "  MEASURE(total_contracted_value) AS contracted,  \n",
        "  MEASURE(invoice_count) AS invoice_count\n",
        "FROM main.finance_lakehouse.metric_cash_obligations\n",
        "GROUP BY ALL"
      ]
    },
    {
      "name": "ds_revenue_growth",
      "displayName": "Revenue Growth Metrics",
      "queryLines": [
        "SELECT   \n",
        "  revenue_year,  \n",
        "  revenue_quarter,  \n",
        "  revenue_month,  \n",
        "  customer,  \n",
        "  legal_entity,  \n",
        "  legal_entity_name,  \n",
        "  contract_status,  \n",
        "  agreement_type,  \n",
        "  MEASURE(recognized_revenue) AS revenue,  \n",
        "  MEASURE(monthly_recurring_revenue) AS mrr,  \n",
        "  MEASURE(annual_recurring_revenue) AS arr,  \n",
        "  MEASURE(billed_amount) AS billed,  \n",
        "  MEASURE(received_amount) AS received,  \n",
        "  MEASURE(transaction_count) AS transactions,  \n",
        "  MEASURE(contract_revenue_at_risk) AS revenue_at_risk\n",
        "FROM main.finance_lakehouse.metric_growth_revenue\n",
        "GROUP BY ALL"
      ]
    }
  ],
