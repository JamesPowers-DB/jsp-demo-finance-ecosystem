{
  "datasets": [
    {
      "name": "ds_headcount_metrics",
      "displayName": "Headcount Productivity Metrics",
      "queryLines": [
        "SELECT ",
        "  year,",
        "  quarter,",
        "  cost_center,",
        "  legal_entity,",
        "  MEASURE(quarterly_headcount_cost) AS headcount_cost,",
        "  MEASURE(quarterly_revenue) AS revenue,",
        "  MEASURE(revenue_per_employee) AS revenue_per_employee,",
        "  MEASURE(revenue_per_sales_employee) AS revenue_per_sales,",
        "  MEASURE(quarterly_spend) AS spend,",
        "  MEASURE(spend_per_employee) AS spend_per_employee",
        "FROM main.finance_lakehouse.metric_headcount",
        "GROUP BY ALL"
      ]
    },
    {
      "name": "ds_variance_performance",
      "displayName": "Variance & Performance Metrics",
      "queryLines": [
        "SELECT ",
        "  fiscal_year,",
        "  fiscal_quarter,",
        "  quarter_name,",
        "  cost_center,",
        "  cost_center_name,",
        "  legal_entity,",
        "  legal_entity_name,",
        "  MEASURE(budget_amount) AS budget,",
        "  MEASURE(actual_amount) AS actuals,",
        "  MEASURE(variance_amount) AS variance,",
        "  MEASURE(variance_percent) AS variance_pct,",
        "  MEASURE(actual_spend) AS actual_spend,",
        "  MEASURE(actual_salary) AS actual_salary",
        "FROM main.finance_lakehouse.metric_variance_performance",
        "GROUP BY ALL"
      ]
    },
    {
      "name": "ds_cash_obligations",
      "displayName": "Cash & Obligations Metrics",
      "queryLines": [
        "SELECT ",
        "  invoice_year,",
        "  invoice_month,",
        "  invoice_status,",
        "  supplier,",
        "  legal_entity,",
        "  contract_status,",
        "  agreement_type,",
        "  MEASURE(total_invoice_amount) AS invoice_amount,",
        "  MEASURE(amount_paid) AS paid,",
        "  MEASURE(outstanding_payables) AS outstanding,",
        "  MEASURE(days_payable_outstanding) AS dpo,",
        "  MEASURE(total_contracted_value) AS contracted,",
        "  MEASURE(committed_vs_spent) AS committed_uninvoiced,",
        "  MEASURE(invoice_count) AS invoice_count",
        "FROM main.finance_lakehouse.metric_cash_obligations",
        "GROUP BY ALL"
      ]
    },
    {
      "name": "ds_revenue_growth",
      "displayName": "Revenue Growth Metrics",
      "queryLines": [
        "SELECT ",
        "  revenue_year,",
        "  revenue_quarter,",
        "  revenue_month,",
        "  customer,",
        "  legal_entity,",
        "  legal_entity_name,",
        "  contract_status,",
        "  agreement_type,",
        "  MEASURE(recognized_revenue) AS revenue,",
        "  MEASURE(monthly_recurring_revenue) AS mrr,",
        "  MEASURE(annual_recurring_revenue) AS arr,",
        "  MEASURE(billed_amount) AS billed,",
        "  MEASURE(received_amount) AS received,",
        "  MEASURE(transaction_count) AS transactions,",
        "  MEASURE(contract_revenue_at_risk) AS revenue_at_risk",
        "FROM main.finance_lakehouse.metric_growth_revenue",
        "GROUP BY ALL"
      ]
    }
  ],
